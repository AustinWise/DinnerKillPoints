@{
    ViewBag.Title = "Index";
}
<h2>
    Index</h2>
<table>
    <thead>
        <tr>
            <th>
                First Name
            </th>
            <th>
                Last Name
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (Person p in Model)
        {
            <tr>
                <td>
                    @Html.ActionLink(p.FirstName, "Detail", new { id = p.Id })
                </td>
                <td>
                    @p.LastName
                </td>
            </tr>
        }
    </tbody>
</table>
